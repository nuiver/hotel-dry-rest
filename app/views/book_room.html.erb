<%= form_for @bookings do |f| %>


   <%= f.label :username %>
   <%= f.text_field :name %>

   <%= f.label :arrivaldate %>
   <%= f.date_field :arrivaldate %>

   <%= f.label :departuredate %>
   <%= f.date_field :departuredate %>

   <%= f.label :room %>
   <%= f.text_field :age %>

   <%= f.label :num_person %>
   <%= f.number_field :image_url %>

   <%= f.submit (@dinosaur.new_record? ? "Create" : "Update") %>
<% end %>
